{
    "name": "santwer/developeranalyst",
    "description": "Gives Laravel Projekt Git stats about the developers",
    "type": "package",
    "require": {
        "laravel/framework": "^8.0|^9.0|^10",
        "php": "^7.4|^8.0|^8.1|^8.2",
        "gitonomy/gitlib": "^1.3",
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Santwer\\DeveloperAnalyst\\": "src/"
        }
    },
    "authors": [
        {
            "name": "santwer"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Santwer\\DeveloperAnalyst\\DeveloperAnalystProvider"
            ]
        }
    }
}
